PUBSUB_EMULATOR_HOST=

# Database Configuration
DB_DRIVER=mongodb
DB_HOST=localhost
DB_PORT=27017
DB_DATABASE=ckg_db
DB_USERNAME=
DB_PASSWORD=
DB_ATTRIBUTES=

